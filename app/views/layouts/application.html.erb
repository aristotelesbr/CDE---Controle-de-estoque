<!DOCTYPE html>
<html>
  <head>
    <title>CDE</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <div class='page'>
      <%= render 'nav' %>
      <%= render 'flash_messages' %>

      <%= yield %>

      <footer>
          <p>
            <%= t('signature') %>
          </p>
      </footer>
    </div>
  </body>
</html>
